{
  "dependencies": {
    "init": "^0.1.2",
    "npm": "^9.3.0"
  },
  "devDependencies": {
    "assemblyscript": "^0.26.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
